	Journal = {bioRxiv 459966; doi: https://doi.org/10.1101/459966},
	Publisher = {Cold Spring Harbor Laboratory},
	Title = {Determinants of {QTL} mapping power in the realized Collaborative Cross},
	Url = {https://www.biorxiv.org/content/early/2018/12/01/459966},
	Year = {2019},

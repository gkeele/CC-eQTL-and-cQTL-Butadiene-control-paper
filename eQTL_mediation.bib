	Issn = {1553-7404},
	Journal = {PLoS genetics},
	Month = {jan},
	Number = {1},
	Pages = {e1004857},
	Pmid = {25569255},
	Title = {{The genetic and mechanistic basis for variation in gene regulation.}},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/25569255 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4287341},
	Volume = {11},
	Year = {2015},
	Bdsk-Url-1 = {http://www.ncbi.nlm.nih.gov/pubmed/25569255%20http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4287341},

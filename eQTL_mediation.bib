	Number = {6},
	Pages = {e1002789},
	Pmid = {22761590},
	Title = {{Extensive evolutionary changes in regulatory element activity during human origins are associated with altered gene expression and positive selection.}},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/22761590 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3386175},
	Volume = {8},
	Year = {2012},
	Bdsk-Url-1 = {http://www.ncbi.nlm.nih.gov/pubmed/22761590%20http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3386175},

	Journal = {Genetics},
	Keywords = {CXCL1,MPP,Multiparent Advanced Generation Inter-Cross (MAGIC,asthma,complex traits,expression quantitative trait locus (eQTL),multiparental populations,neutrophil,systems genetics},
